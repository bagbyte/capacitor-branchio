{
  "name": "@bagbyte/capacitor-branchio",
  "version": "0.0.1",
  "description": "Capacitor plugin for web, iOS and Android for using native Branch.io SDK",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Sabino Papagna",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "@types/node": "^12.6.8",
    "branch-sdk": "^2.50.2",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "src/",
    "CapacitorBranchio.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    },
    "web": {
      "src": "src"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bagbyte/capacitor-branchio.git"
  },
  "bugs": {
    "url": "https://github.com/bagbyte/capacitor-branchio.git/issues"
  }
}
